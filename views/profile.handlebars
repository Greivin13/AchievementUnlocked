<main>
    {{#each userProfile as |userProfile|}}
  {{#if userProfile.userProfile}}
  {{#each userProfile.userProfile as |userProfile|}}
  <div id="profile">
  <h2>{{review.title}}</h2>
  <p>{{review.review_content}}</p>
</div>
  {{/each}}
  {{/if}}
  {{/each}}
</main>

<div id="userProfile" class="container card col-3">
  <div class="card-header">
    {{#each displayData as |review|}}
      <div>
        <ul class="list-group list-group-flush">
          <li class="list-group-item">Title:
            {{review.title}}
            (id:
            {{review.id}})</li>
          <li class="list-group-item">Review: {{review.review_content}}</li>
          <li class="list-group-item">Date: {{review.created_at}}</li>
        </ul>
        <hr />
      </div>
    {{/each}}
  </div>
</div>

