<div class="form-signin container col-3 text-center">
  <form>
    <img class="my-4 w-100" src="https://i.imgur.com/Mt6nYs0.png" alt="logo" />
    <h1 class="h3 mb-3 fw-normal">Create an account</h1>

    <div class="form-floating">
      <input
        type="email"
        class="form-control"
        id="floatingInput"
        placeholder="name@example.com"
      />
      <label for="floatingInput">Email address</label>
    </div>
    <div class="form-floating">
      <input
        type="password"
        class="form-control"
        id="setPassword"
        placeholder="Password"
      />
      <label for="setPassword">Password</label>
    </div>
    <div class="form-floating">
      <input
        type="password"
        class="form-control"
        id="confirmPassword"
        placeholder="Password"
      />
      <label for="confirmPassword">Confirm Password</label>
    </div>

    <button class="mt-3 w-100 btn btn-lg btn-primary" type="submit">Sign up!</button>
  </form>
  <button id="loginBtn" class="mt-4 btn btn-primary" type="submit">Back to login</button>
</div>